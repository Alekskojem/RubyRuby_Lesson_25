
Cars
----------

CREATE TABLE Cars (
	id INTEGER PRIMARY KEY AUTOINCREMENT,
	name VARCHAR (50),
	price INTEGER (10));


	Add Name Price
	----------------
	INSERT INTO Cars (name, price)
	VALUES ('BMW', '90000');

